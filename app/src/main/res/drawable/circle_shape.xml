<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval" >
            <solid android:color="#D3D3D3"/>
            <stroke android:color="#000000" android:width="1dp"/>
            <size android:width="200dp" android:height="200dp"/>
        </shape>
    </item>
    <!--Left Arrow-->
    <item android:gravity="left" android:left="10dp">
        <shape android:shape="line">
            <solid android:color="#D3D3D3"/>
            <stroke android:color="#000000" android:width="2dp" android:dashGap="2dp" android:dashWidth="1dp"/>
            <size android:width="20dp" android:height="21dp"/>
        </shape>
    </item>
    <!--Right Arrow-->
    <item android:gravity="right" android:right="10dp">
        <shape android:shape="line">
            <solid android:color="#D3D3D3"/>
            <stroke android:color="#000000" android:width="2dp" android:dashGap="2dp" android:dashWidth="1dp"/>
            <size android:width="25dp" android:height="21dp"/>
        </shape>
    </item>
    <!--Up Arrow-->
    <item android:gravity="center" android:bottom="160dp">
        <rotate android:fromDegrees="90">
            <shape android:shape="line">
                <solid android:color="#D3D3D3"/>
                <stroke android:color="#000000" android:width="2dp" android:dashGap="2dp" android:dashWidth="1dp"/>
                <size android:width="25dp" android:height="21dp"/>
            </shape>
        </rotate>
    </item>

    <!--Down Arrow-->
    <item android:gravity="center" android:top="160dp">
        <rotate android:fromDegrees="90">
            <shape android:shape="line">
                <solid android:color="#D3D3D3"/>
                <stroke android:color="#000000" android:width="2dp" android:dashGap="2dp" android:dashWidth="1dp"/>
                <size android:width="25dp" android:height="21dp"/>
            </shape>
        </rotate>
    </item>
    <item
        android:top="20dp"
        android:bottom="20dp"
        android:right="20dp"
        android:left="20dp">
        <shape android:shape="oval" >
            <solid android:color="#000000"/>
            <stroke android:color="#FFFFFF" android:width="1dp"/>
            <size android:width="6dp" android:height="6dp"/>
        </shape>
    </item>
</layer-list>